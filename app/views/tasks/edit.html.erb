<%= form_with model: @task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.check_box :completed, {}, "true", "false" %>
  <%= f.label :completed, "Completed" %>
  <%= f.submit %>
<% end %>



<%= link_to fa_icon("rotate-left", text:"Back to list"), tasks_path, class:"text-decoration-none" %>
